{
  "name": "play-midnight-redux",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-eslint": "7.2.3",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "^6.26.0",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "lodash": "^4.17.4",
    "parcel-bundler": "^1.3.1",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "styled-components": "^2.4.0",
    "typeface-roboto": "^0.0.45"
  },
  "scripts": {
    "start":
      "parcel build src/background.js --out-dir dist && parcel src/index.html",
    "build":
      "parcel build src/index.js --out-dir build && parcel build src/background.js --no-minify --out-dir build"
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./src"]
        }
      ],
      ["transform-decorators-legacy"]
    ],
    "presets": ["react-app"]
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "chrome": true
    }
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4"
  }
}
